        <Rectangle HorizontalAlignment="Center" Height="50" Margin="0,374,0,0" Stroke="Black" VerticalAlignment="Top" Width="780" Fill="#FF91B0AB"/>
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="760,0,0,0" Width="40" Height="30"/>
        <Button Content="Button" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="720,0,0,0" Width="40" Height="30"/>

    </Grid>
</Window>
